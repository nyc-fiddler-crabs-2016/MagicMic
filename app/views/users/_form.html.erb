
<h1>Sign Up</h1>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <br>
  <%= f.text_field :username, autofocus: true %>
  <br>
  <%= f.label :password %>
  <br>
  <%= f.password_field :password %>
  <br>
  <%= f.label :email %>
  <br>
  <%= f.text_field :email %>
  <br>
  <%= f.submit "Submit" %>

<% end %>
